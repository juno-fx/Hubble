{
  "name": "hubble",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "latest",
    "bootstrap": "latest",
    "keycloak-js": "latest",
    "react": "^17.0.1",
    "react-bootstrap": "latest",
    "react-bootstrap-icons": "^1.3.0",
    "react-dom": "latest",
    "react-router": "^5.2.0",
    "react-router-dom": "latest",
    "react-spinners": "^0.10.4",
    "sass": "latest"
  },
  "scripts": {
    "start": "webpack serve --config ./webpack.config.js --mode development --progress",
    "build": "webpack --mode production --progress"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "babel-loader": "^8.2.2",
    "copy-webpack-plugin": "^7.0.0",
    "css-loader": "^5.0.1",
    "react-hot-loader": "^4.13.0",
    "style-loader": "^2.0.0",
    "webpack": "^5.18.0",
    "webpack-cli": "^4.4.0",
    "webpack-dev-server": "^3.11.2"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties"
    ]
  }
